msgstr ""
"]\n"
"               [PAD...] [EXPRESSIE]\n"

#: find/util.c:206
#, c-format
msgid "Warning: file %s appears to have mode 0000"
msgstr "Waarschuwing: bestand %s lijkt modus 0000 te hebben."

#: find/util.c:754
#, c-format
msgid "Ignoring unrecognised debug flag %s"
msgstr "Onbekende debugvlag '%s'; genegeerd."

#: find/util.c:761
msgid "Empty argument to the -D option."
msgstr "Argument van optie '-D' is leeg"

#: find/util.c:775
msgid "The -O option must be immediately followed by a decimal integer"
msgstr "Direct na optie '-O' dient een decimaal getal te staan"

#: find/util.c:784 find/util.c:794
