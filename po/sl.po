#: find/parser.c:555
msgid ""
"default path is the current directory; default expression is -print\n"
"expression may consist of:\n"
"operators (decreasing precedence; -and is implicit where no others are given):\n"
"      ( EXPR ) ! EXPR -not EXPR EXPR1 -a EXPR2 EXPR1 -and EXPR2\n"
msgstr ""
"privzeta pot je trenutni imenik; privzeti izraz je -print\n"
"izraz lahko sestavljajo:\n"
"operatorji (padajoèi prednost; èe ni podano niè drugega, se privzame -and):\n"
"      ( IZRAZ ) ! IZRAZ -not IZRAZ IZRAZ1 -a IZRAZ2 IZRAZ1 -and -IZRAZ2\n"

#: find/parser.c:560
msgid ""
"      EXPR1 -o EXPR2 EXPR1 -or EXPR2 EXPR1 , EXPR2\n"
"options (always true): -daystart -depth -follow --help\n"
"      -maxdepth LEVELS -mindepth LEVELS -mount -noleaf --version -xdev\n"
"tests (N can be +N or -N or N): -amin N -anewer FILE -atime N -cmin N\n"
msgstr ""
"      IZRAZ1 -o IZRAZ2 IZRAZ1 -or IZRAZ2 IZRAZ1 , IZRAZ2\n"
"izbire (vedno vklapljamo): -daystart -depth  -follow --help\n"
"      -maxdepth RAVNI -mindepth RAVNI -mount -noleaf --version -xdev\n"
"testi (N can be +N or -N or N): -amin N -anewer DATOTEKA -atime N -cmin N\n"

#: find/parser.c:565
msgid ""
"      -cnewer FILE -ctime N -empty -false -fstype TYPE -gid N -group NAME\n"
"      -ilname PATTERN -iname PATTERN -inum N -ipath PATTERN -iregex PATTERN\n"
"      -links N -lname PATTERN -mmin N -mtime N -name PATTERN -newer FILE\n"
msgstr ""
"      -cnewer DATOTEKA -ctime N -empty -false -fstype TIP -gid N -group IME\n"
"      -ilname VZOREC -iname VZOREC -inum N -ipath VZOREC -iregex VZOREC\n"
"      -links N -lname VZOREC -mmin N -mtime N -name VZOREC -newer DATOTEKA\n"

#: find/parser.c:569
msgid ""
"      -nouser -nogroup -path PATTERN -perm [+-]MODE -regex PATTERN\n"
"      -size N[bckw] -true -type [bcdpfls] -uid N -used N -user NAME\n"
"      -xtype [bcdpfls]\n"
msgstr ""
"      -nouser -nogroup -path VZOREC -perm [+-]ZA©ÈITA -regex VZOREC\n"
"      -size N[bckw] -true -type [bcdpfls] -uid N -used N -user IME\n"
"      -xtype [bcdpfls]\n"

#: find/parser.c:573
msgid ""
